{
  "name": "divi-child",
  "version": "1.0.0",
  "description": "Webscroll divi child theme",
  "author": "Webscroll Inc.",
  "scripts": {
    "css:watch": "sass --no-source-map --watch --style=compressed ./src/styles/index.scss:./style.css",
    "css:build": "sass --no-source-map --style=expanded ./src/styles/index.scss:./style.css",
    "wpb:build": "webpack --config webpack.config.js",
    "wpb:watch": "webpack --config webpack.config.js --watch",
    "build:all": "npm run css:build && npm run wpb:build"
  },
  "dependencies": {
    "prettier": "^3.1.1"
  },
  "devDependencies": {
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}